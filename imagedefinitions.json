{
    "family": "springworks-nodejs-task",
    "containerDefinitions": [
        {
            "image": "577263614154.dkr.ecr.us-east-1.amazonaws.com/springworks-nodejs:latest",
            "name": "springworks-nodejs-task-service",
            "cpu": 0,
            "memory": 1024,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort":80
                }
            ],
	    "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/springworks-nodejs-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
           "environment": [
         ]

        }
    ]
}